{
    "servers": {
        "github": {
            "type": "http",
            "url": "https://api.githubcopilot.com/mcp/",
            "gallery": true,
            "version": "0.0.1"
        },
        "huggingface": {
            "type": "http",
            "url": "https://hf.co/mcp",
            "gallery": true,
            "version": "0.0.1"
        },
        "markitdown": {
            "type": "stdio",
            "command": "uvx",
            "args": [ "markitdown-mcp" ],
            "gallery": true,
            "version": "0.0.1"
        },
        "sequentialthinking": {
            "type": "stdio",
            "command": "npx",
            "args": [ "-y", "@modelcontextprotocol/server-sequential-thinking@latest" ],
            "gallery": true,
            "version": "0.0.1"
        },
        "memory": {
            "type": "stdio",
            "command": "npx",
            "args": [ "-y", "@modelcontextprotocol/server-memory@latest" ],
            "env": {
                "MEMORY_FILE_PATH": "$C:/Users/jkill/OneDrive/Desktop/JDW-DEV/GitHub/JDwinkle/PYTHON/.mcp/memory.json"
            },
            "gallery": true,
            "version": "0.0.1"
        }
    },
    "inputs": [
        {
            "id": "serena_context",
            "type": "promptString",
            "description": "Serena context to use (e.g., ide-assistant).",
            "password": false
        },
        {
            "id": "serena_project_abs_path",
            "type": "promptString",
            "description": "Absolute path to the project root Serena should index.",
            "password": false
        }
    ]
}
